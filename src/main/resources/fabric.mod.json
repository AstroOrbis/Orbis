{
  "schemaVersion": 1,
  "id": "Orbis",
  "version": "${version}",

  "name": "Orbis Client",
  "description": "Orbis utility/hacked client.",
  "authors": [
    "Orbis client: AstroOrbis",
    "Meteor client: MineGame159",
    "Meteor client: squidoodly",
    "Meteor client: seasnail"
  ],
  "contact": {
    "Meteor homepage": "https://meteorclient.com",
    "issues": "https://github.com/AstroOrbis/Orbis/issues",
    "sources": "https://github.com/MeteorDevelopment/meteor-client",
    "discord": "AstroOrbis#0623",
     "Twitter": "https://twitter.com/AstroOrbis"
  },

  "license": "GPL-3.0",
  "icon": "assets/meteor-client/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "minegame159.meteorclient.MeteorClient"
    ]
  },
  "mixins": [
    "meteor-client.mixins.json",
    "meteor-client-sodium.mixins.json"
